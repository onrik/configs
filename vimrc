set nocompatible
set number

filetype plugin indent on

set tabstop=4
set shiftwidth=4
set showtabline=0
set smarttab
set expandtab
set softtabstop=4
set autoindent
set mouse=a

let python_highlight_all = 1
let javascript_higlight_all = 1
set t_Co=256

syntax on
set backspace=indent,eol,start whichwrap+=<,>,[,]
